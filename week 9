#include <stdio.h>

// Declaration of struct type node
struct node {
  int data;
  struct node *next;
};

// Declaration of struct type Queue
struct Queue {
#include <stdio.h>
#include <stdlib.h>
// Declaration of struct type node
struct node {
  int data;
  struct node *next;
};

// Declaration of struct type Queue
struct Queue {
  struct node *front;
  struct node *rear;
};

// function to insert the element in the Queue
void enqueue(struct Queue *q, int item) {
  struct node *newnode = (struct node *) malloc (sizeof(struct node));
  newnode->data = item;
  newnode->next = NULL;

  if (q->rear == NULL) {
    q->front = q->rear = newnode;
  } else {
    q->rear->next = newnode;
    q->rear = newnode;
  }
}

// function to delete the element from the queue
int dequeue(struct Queue *q) {
  if (q->front == NULL) {
    printf("\nQueue is empty");
    return -1;
  }

  int item = q->front->data;
  struct node *temp = q->front;
  q->front = q->front->next;
  free(temp);

  if (q->front == NULL) {
    q->rear = NULL;
  }

  return item;
}

// function to get the front of the queue
int peek(struct Queue *q) {
  if (q->front == NULL) {
    printf("\nQueue is empty");
    return -1;
  }

  return q->front->data;
}

// function to display all the elements of the queue
void display(struct Queue *q) {
  if (q->front == NULL) {
    printf("\nQueue is empty");
    return;
  }

  struct node *temp = q->front;
  while (temp != NULL) {
    printf("%d ", temp->data);
    temp = temp->next;
  }

  printf("\n");
}

int main() {
  struct Queue q;
  q.front = NULL;
  q.rear = NULL;

  int choice, item;

  while (1) {
    printf("1. Enqueue\n2. Dequeue\n3. Display\n4. Delete\n5. Exit\n");
    printf("Enter your choice: ");
    scanf("%d", &choice);

    switch (choice) {
      case 1:
        printf("Enter item to enqueue: ");
        scanf("%d", &item);
        enqueue(&q, item);
        break;
      case 2:
        item = dequeue(&q);
        if (item != -1) {
          printf("Dequeued item: %d\n", item);
        }
        break;
      case 3:
        display(&q);
        break;
      case 4:
        dequeue(&q);
        printf("Queue deleted\n");
        break;
      case 5:
        exit(0);
      default:
        printf("Invalid choice\n");
        break;
    }
  }

  return 0;
}



